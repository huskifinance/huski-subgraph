{
  "name": "huski-subgraph",
  "description": "HuskiFinance Subgraph",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:huskifinance/huski-subgraph.git",
  "author": "HuskiFinance",
  "workspaces": [
    "subgraphs/**/*"
  ],
  "scripts": {
    "codegen": "lerna run codegen",
    "build": "lerna run build",
    "format:check": "prettier --check '*/**/*.{js,ts}'",
    "format:write": "prettier --write '*/**/*.{js,ts}'",
    "lint": "eslint '*/**/*.{js,ts}'",
    "prepare": "huski install"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphprotocol/graph-cli": "^0.22.1",
    "@graphprotocol/graph-ts": "^0.22.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "lerna": "^4.0.0",
    "prettier": "^2.4.0",
    "typescript": "^4.3.5"
  }
}
